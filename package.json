{
  "name": "@rgrannell/kohl",
  "version": "0.1.0",
  "description": "Highlight Logs",
  "author": "Róisín Grannell <r.grannell2@gmail.com>",
  "scripts": {
    "docs": "npx typedoc --out docs src",
    "tsc": "tsc",
    "start": "npm run tsc && cat data/apache_logs.txt | node dist/cli/kohl.js",
    "standard": "npx standard 'src/**'",
    "standard:fix": "npx standard 'src/**' --fix",
    "test": "npx test --coverage-report=lcov",
    "perf:measure": "cat data/apache_logs.txt | node --prof dist/cli/kohl.js",
    "perf:clean": "rm isolate* || echo ''",
    "perf:show": "node --prof-process --preprocess -j isolate*.log | node_modules/.bin/flamebearer",
    "perf": "npm run perf:clean; npm run perf:measure; npm run perf:show"
  },
  "preferGlobal": true,
  "bin": {
    "kohl": "dist/cli/kohl.js"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rgrannell1/kohl.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rgrannell1/kohl/issues"
  },
  "homepage": "https://github.com/rgrannell1/kohl#readme",
  "devDependencies": {
    "flamebearer": "^1.1.3",
    "standard": "13.1.0",
    "typedoc": "^0.19.2"
  },
  "dependencies": {
    "@types/ansi-styles": "^3.2.1",
    "@types/docopt": "^0.6.31",
    "@types/parsimmon": "^1.10.3",
    "@types/split": "^1.0.0",
    "@types/through": "0.0.30",
    "ansi-styles": "^2.2.1",
    "chalk": "^4.1.0",
    "docopt": "^0.6.2",
    "general-category": "^1.6.0",
    "ink": "^3.0.6",
    "keypress": "^0.2.1",
    "moment": "2.26.0",
    "nanoid": "^3.1.9",
    "neodoc": "2.0.2",
    "parjs": "^0.12.7",
    "parsimmon": "^1.16.0",
    "react": "^16.13.1",
    "split": "^1.0.1",
    "strip-ansi-stream": "1.0.0",
    "through": "^2.3.8",
    "typescript": "^4.0.3",
    "unicode": "^12.1.0"
  }
}
