{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "preLaunchTask": "npm: build",
      "name": "Launch Kohl",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "args": [
        "<",
        "data/simple.txt"
     ],
      "program": "${workspaceFolder}/dist/src/cli/kohl.js",
      "console": "integratedTerminal"
    }
  ]
}